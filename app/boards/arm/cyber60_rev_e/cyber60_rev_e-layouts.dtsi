/ {
    layout_ansi_position_map: layout_ansi_position_map {
        compatible = "zmk,physical-layout-position-map";
    };
};

&layout_ansi_position_map {
    layout_ansi_posmap: layout_ansi {
        physical-layout = <&layout_ansi>;
        positions
            = <1  0  4  6  8  10 12>     // Row 1 (even columns)
            , <2  3  5  7  9  11 13>     // Row 2 (odd columns)
            , <14 16 18 20 22 24 26>     // Row 3 (even columns)
            , <15 17 19 21 23 25 27>     // Row 4 (odd columns)
            , <28 30 32 34 36 38 40>     // Row 5 (even columns)
            , <29 31 33 35 37 39 41>     // Row 6 (odd columns)
            , <42 44 46 48 50 52 54>     // Row 7 (even columns)
            , <43 45 47 49 51 53 55>     // Row 8 (odd columns)
            , <56 58 60 62 64 66 68>     // Row 9 (even columns)
            , <57 59 61 63 65 67 69>     // Row 10 (odd columns)
            ;
    };
};

